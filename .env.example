# Ghostnet Environment Variables
# Copy this to .env and fill in the values

# 32-character encryption key (Base64 or Plaintext)
GHOSTNET_ENCRYPTION_KEY=your-secure-32-byte-key-here

# Server configuration
GHOSTNET_SERVER_IP=127.0.0.1
GHOSTNET_SERVER_PORT=53

# Domain for DNS tunneling
GHOSTNET_DOMAIN=ghost.net
